{
  "name": "gatling-js-demo",
  "version": "3.13.501",
  "private": true,
  "type": "module",
  "main": "target/bundle",
  "dependencies": {
    "@gatling.io/core": "3.14.306",
    "@gatling.io/http": "3.14.306",
    "axios": "^1.9.0",
    "dotenv": "^16.5.0"
  },
  "devDependencies": {
    "@gatling.io/cli": "3.14.306",
    "prettier": "3.5.3",
    "rimraf": "6.0.1"
  },
  "scripts": {
    "clean": "rimraf target",
    "format": "prettier --write \"**/*.js\"",
    "build": "gatling build",
    "recorder": "gatling recorder",
    "setup": "node gatling-helper.js --setup-data",
    "get-token": "node gatling-helper.js --get-token",
    "generate-config": "node gatling-helper.js --generate-config",
    "test": "npm run setup && npx gatling run --simulation baseline",
    "test-simple": "npm run generate-config && npx gatling run --simulation baseline",
    "test-full": "node gatling-helper.js --full-setup && npx gatling run --simulation baseline"
  }
}
